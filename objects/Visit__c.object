    <fields>
        <fullName>Visit_Duration__c</fullName>
        <deprecated>false</deprecated>
        <description>Time elapsed between Check-In and Check-Out</description>
        <externalId>false</externalId>
        <formula>TEXT(FLOOR((Check_out_Date_Time__c - Check_in_Date_Time__c)*24))
&amp;&apos; h &apos;&amp;
TEXT(
ROUND(
MOD(
(Check_out_Date_Time__c - Check_in_Date_Time__c)*24*60, 60
), 0
)
)
&amp;&apos; min&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Visit Duration</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
