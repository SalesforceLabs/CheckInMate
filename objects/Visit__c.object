    <fields>
        <fullName>Compliance_Now__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(Check_in_Distance__c &gt; $CustomMetadata.Visit_Compliance__mdt.Default.Max_Distance__c || Check_out_Distance__c &gt; $CustomMetadata.Visit_Compliance__mdt.Default.Max_Distance__c, IMAGE(&quot;/resource/CheckIn_CMV__error_icon_non_compliant&quot;, &quot;Error red X&quot;, 25, 25), IMAGE(&quot;/resource/CheckIn_CMV__success_icon_compliant&quot;, &quot;Green Check&quot;, 25, 25))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Shows if visit is under compliance considering the current compliance rules.</inlineHelpText>
        <label>Compliance Now</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Compliance_on_Visit__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
    Check_in_Distance__c &gt; Check_in_Max_Distance_Compliance__c
    ||
    Check_out_Distance__c &gt; Check_out_Max_Distance_Compliance__c,
    IMAGE(&quot;/resource/CheckIn_CMV__error_icon_non_compliant&quot;, &quot;Error red X&quot;, 25, 25),
    IMAGE(&quot;/resource/CheckIn_CMV__success_icon_compliant&quot;, &quot;Green Check&quot;, 25, 25)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Shows if visit is under compliance considering the compliance rules when the visit was done.</inlineHelpText>
        <label>Compliance on Visit</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
